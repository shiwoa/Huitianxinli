<?xml version="1.0" encoding="utf-8"?>
<!-- 解除第三方登录的布局 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_F6"
    android:orientation="vertical" >

    <include layout="@layout/login_register_titlelayout" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y8"
        android:background="@color/White"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/qq_relat"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y31"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/QQImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x12"
                android:background="@drawable/qqimagenot" />

            <TextView
                android:id="@+id/QQText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x6"
                android:layout_toRightOf="@id/QQImage"
                android:textColor="@color/color_7c"
                android:textSize="@dimen/x12" />

            <LinearLayout
                android:id="@+id/QQBinding_linear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x12"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/QQBinding"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x16"
                    android:text="@string/immediately_binding"
                    android:textColor="@color/color_83"
                    android:textSize="@dimen/x12" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/binding_go" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/x12"
            android:layout_marginRight="@dimen/x12"
            android:background="@color/color_F6" >
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/sina_relat"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y31"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/sinaImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x12"
                android:background="@drawable/sinaimagenot" />

            <TextView
                android:id="@+id/sinaText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x6"
                android:layout_toRightOf="@id/sinaImage"
                android:textColor="@color/color_7c"
                android:textSize="@dimen/x12" />

            <LinearLayout
                android:id="@+id/sinaBinding_linear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x12"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/sinaBinding"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x16"
                    android:text="@string/immediately_binding"
                    android:textColor="@color/color_83"
                    android:textSize="@dimen/x12" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/binding_go" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="@dimen/x12"
            android:layout_marginRight="@dimen/x12"
            android:background="@color/color_F6" >
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/weixin_relat"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y31"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/weixinImage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x12"
                android:background="@drawable/weixinimagenot" />

            <TextView
                android:id="@+id/weixinText"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x6"
                android:layout_toRightOf="@id/weixinImage"
                android:textColor="@color/color_7c"
                android:textSize="@dimen/x12" />

            <LinearLayout
                android:id="@+id/weixinText_linear"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/x12"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/weixinBinding"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/x16"
                    android:text="@string/immediately_binding"
                    android:textColor="@color/color_83"
                    android:textSize="@dimen/x12" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/binding_go" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/view" />
    </LinearLayout>

</LinearLayout>